/* miniprogram/pages/HelpFeedback/HelpFeedback.wxss */
page {
  background: #f6f6f6;
}
/* 字体图标 */
@import "../../css/iconfont.wxss";

.HelpFeedback-top{
    background: #fff;
}
.HelpFeedback-bottom{
  background: #fff;
  margin-top:10px;
}
.help,.help-foot{
  width:90%;
  margin: 0 auto;
}

.help-intro{
  height:50px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eaeaea;
}
.icon-you1{
  color:#bbb;
}
.help-intro:nth-last-child(1){
  border-bottom:none;
}
